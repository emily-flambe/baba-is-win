{
  "name": "baba-is-win",
  "compatibility_date": "2024-11-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "./dist/_worker.js/index.js",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "baba-is-win-db",
      "database_id": "YOUR_DATABASE_ID"
    }
  ],
  "vars": {
    "JWT_SECRET": "YOUR_JWT_SECRET"
  }
}
